  "version": "1.0.0",
  "homepage": "https://github.com/JacobeanRnD/SCXMLD",
  "main": "./index.js",
  "scripts": {
    "test": "bash run-tests.sh",
    "postinstall": "bower install",
    "start": "node index.js"
  },
